<Application x:Class="WpfAppListUsers.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfAppListUsers"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/ListItemDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/MessageItemDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/AvatarDictionary.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/TopBorderButtonStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/WindowsStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
